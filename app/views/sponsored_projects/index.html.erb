
<h1>Sponsored Projects</h1>
<table cellpadding="3">
  <tr><th>Sponsor</th><th>Name</th><th>&nbsp;</th></tr>
  <% @projects.each do |project| %>
      <tr>
        <td><%= project.sponsor.name %></td>
        <td><%= project.name %></td>
        <td><%= link_to 'Edit', edit_sponsored_project_path(project) %></td>
      </tr>
  <% end %>
<tr><td colspan="3"><%= link_to 'New project', new_sponsored_project_path %></td></tr>
</table>


<h1>Sponsors</h1>
<table cellpadding="3">
  <tr><th>Name</th></tr>
  <% @sponsors.each do |sponsor| %>
      <tr>
        <td><%= sponsor.name %></td>
      </tr>

  <% end %>
</table>
<br/>
<br/>